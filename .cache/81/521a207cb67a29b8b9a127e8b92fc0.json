\n    justify-self:center;\r\n    animation:rotateLogo 5s cubic-bezier(.36,.07,.19,.97) infinite;\r\n\r\n}\r\n\r\n.ay1 {\r\n    \r\n}\r\n\r\n.logo{\r\n    animation:rotateLogo 5s cubic-bezier(.36,.07,.19,.97) infinite;\r\n\r\n}\r\n\r\n#svg-1 {\r\n    color:white;\r\n}\r\n\r\n@keyframes rotateLogo {\r\n    0% {transform: rotate(0deg);}\r\n    25% { transform: rotate(-2deg);}\r\n    50% {\r\n        transform: rotate(-2deg);\r\n        \r\n    }\r\n    100% {transform: rotate(0deg);}\r\n}\r\n\r\n#div-clouds {\r\n    position:fixed;\r\n    width:105%;\r\n    top:70%;\r\n}\r\n\r\n#div-years {\r\n    position:fixed;\r\n    width:100%;\r\n    height:80vh;\r\n    margin-top:2vmin;\r\n    grid-column: 1/2;\r\n    align-items:center;\r\n    justify-items:center;\r\n    display:grid;\r\n    grid-template-columns: 1fr 5%;\r\n    grid-template-rows:repeat(auto-fit, 3vmin);\r\n\r\n    /* active class for years when scrolling */\r\n    .active {\r\n        color:white;\r\n        font-size:4vmin;\r\n    }\r\n\r\n    .nonactive {\r\n        color:#232b2b;\r\n        font-size:3vmin;\r\n    }\r\n\r\n    .nonactive-dot {\r\n        background-color:#232b2b;\r\n        height: 1vmin;\r\n        width: 1vmin;\r\n    }\r\n\r\n    .active-dot {\r\n        background-color:white;\r\n        height: 1.5vmin;\r\n        width: 1.5vmin;\r\n    }\r\n    \r\n    .dot {\r\n        grid-column: 2/3;\r\n        height: 1vmin;\r\n        width: 1vmin;\r\n        cursor:pointer;\r\n        border-radius: 50%;\r\n        &:hover {\r\n            background-color:#ed1c24;\r\n            height: 1.5vmin;\r\n            width: 1.5vmin;\r\n            \r\n        }\r\n      }\r\n\r\n    \r\n\r\n    p {\r\n        grid-column: 2/3;\r\n        cursor:pointer;\r\n        font-family: LL-Reg;\r\n        font-size:3vmin;       \r\n        -webkit-transition: font-size 5s smooth;\r\n        -moz-transition: font-size 5s smooth;\r\n        -o-transition: font-size 5s smooth;\r\n        transition: font-size 5s smooth;\r\n\r\n        &:hover {\r\n            font-size:4.2vmin;\r\n            color:#ed1c24;\r\n            \r\n        }\r\n    }\r\n}\r\n\r\n\r\n\r\n#div-clouds svg {\r\n    width:100%;\r\n    height:auto;\r\n}\r\n\r\n/* SILICON VALLEY LOGO SVG STYLING   */\r\n.ay1 {\r\n    font-size: 200px;\r\n    fill: #ed1c24;\r\n    stroke: #fff;\r\n    stroke-miterlimit: 10;\r\n    stroke-width: 4px;\r\n    font-family: BebasNeue-Regular, Bebas Neue;\r\n  }\r\n  .ay2 {\r\n    letter-spacing: 0em;\r\n  }\r\n  .ay3 {\r\n    letter-spacing: 0em;\r\n  }\r\n  .ay4 {\r\n    letter-spacing: -0.03em;\r\n  }\r\n\r\n  /* styling timeline div */\r\n#timeline-div {\r\n    display:grid;\r\n    width:100%;\r\n    align-items:center;\r\n    \r\n    grid-template-rows:repeat(14, 100vh);\r\n    \r\n}\r\n\r\n#timeline-div .timeline-block {\r\n    width:100%;\r\n    height:100%;\r\n    display:grid;\r\n    justify-items:center;\r\n    align-items:center;\r\n    grid-template-columns:1fr 1fr 1fr 1fr;\r\n    grid-template-rows:1fr 1fr 1fr 1fr;\r\n\r\n\r\n}\r\n\r\n.logo {\r\n    grid-column:2/4;\r\n    grid-row:2/4;\r\n    height:50%;\r\n    width:auto;\r\n    cursor:pointer;\r\n}\r\n\r\n.logo img{\r\n    width:auto;\r\n    height:100%;\r\n}\r\n\r\n.year {\r\n    grid-column:4/4;\r\n    grid-row:4/4;\r\n}\r\n\r\n.year p{\r\n font-size:5vmin;\r\n}\r\n\r\ni {\r\n    font-size:30vmin;\r\n}\r\n\r\n\r\n// styling info boxes //\r\n\r\n.info-div {\r\n    background-color:white;\r\n    display:none;\r\n    align-items:center;\r\n    justify-items:center;\r\n    opacity:0;\r\n   \r\n    width:100%;\r\n    height:40%;\r\n    grid-column:2/4;\r\n    grid-row:2/4;\r\n    border-radius:50px;\r\n\r\n    p {\r\n        margin:5vmin;\r\n        font-family: 'Open Sans', sans-serif;\r\n        font-size:3vmin;\r\n    }\r\n}\r\n\r\n  "},"lineCount":null}},"error":null,"hash":"65bae791c8effc1fcf1b78193db41830","cacheData":{"env":{}}}